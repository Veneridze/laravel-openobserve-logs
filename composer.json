{
    "name": "veneridze/laravel-openobserve-logs",
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.18"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Veneridze\\LaravelOpenobserveLogs\\LogsHandlerProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Veneridze\\LaravelOpenobserveLogs\\": "src/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "baseline": "vendor/bin/phpstan analyse --generate-baseline",
        "test": "vendor/bin/pest"
    },
    "authors": [
        {
            "name": "Veneridze",
            "email": "viacheslav.kopaev@yandex.ru"
        }
    ]
}